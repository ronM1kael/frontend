{
  "expo": {
    "name": "reach",
    "slug": "reach",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Appicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/res.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/res2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ronm1kael.reach"
    },
    "web": {
      "favicon": "./assets/res2.png"
    },
    "scheme": [
      "MyGoogleApp"
    ],
    "extra": {
      "eas": {
        "projectId": "ebce5765-2e58-4a1c-9cee-27b63ba0fee3"
      }
    },
    "plugins": [
      "@react-native-google-signin/google-signin"
    ]
  }
}
