{
  "expo": {
    "name": "reach",
    "slug": "reach",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/finalsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/finalicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ronm1kael.reach"
    },
    "web": {
      "favicon": "./assets/finalfavicon.png"
    },
    "scheme": [
      "MyGoogleApp"
    ],
    "extra": {
      "eas": {
        "projectId": "5ee8189c-9000-4ab1-bfd9-a3bd7d77faf4"
      }
    },
    "plugins": [
      "@react-native-google-signin/google-signin"
    ]
  }
}
